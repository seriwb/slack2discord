{
  "name": "slack2discord",
  "version": "1.0.0",
  "description": "forward slack messages to discord",
  "author": "seri <seri.wb@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "FORCE_COLOR=true next dev | pino-pretty -tc",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "ncu": "ncu",
    "ncuu": "ncu -u",
    "ncum": "ncu --target minor",
    "ncumu": "ncu --target minor -u"
  },
  "dependencies": {
    "@slack/bolt": "3.17.1",
    "discord.js": "14.14.1",
    "next": "14.1.0",
    "pino": "8.17.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zlib-sync": "0.1.9"
  },
  "devDependencies": {
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "browserify-zlib": "0.2.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "import-sort-parser-typescript": "6.0.0",
    "npm-check-updates": "16.14.12",
    "pino-pretty": "10.3.1",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "packageManager": "yarn@4.0.2"
}
